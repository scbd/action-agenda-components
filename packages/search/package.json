    "build": "DEBUG=TRUE dist-builder && DEBUG=TRUE dist-builder widget",
    "build-widget": "dist-builder widget",
    "build-test-widget": "DEBUG=TRUE dist-builder testWidget",
    "serve-widget-test": "dist-builder serveTestWidget",
