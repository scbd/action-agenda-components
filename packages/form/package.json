    "@houlagins/dist-builder": "1.0.11",
    "@scbd/www-css": "1.0.2",
    "fs-extra": "^10.0.0",
    "node-sass": "8.0.0",
    "postcss": "8.4.21",
    "postcss-loader": "7.1.0",
    "sass-loader": "13.2.1",
    "vue": "2.6.11",
    "vue-template-compiler": "2.6.11"
  },
  "description": "https://github.com/scbd/action-agenda-components/tree/master/packages/form#readme",
  "bugs": {
    "url": "https://github.com/scbd/action-agenda-components/issues"
  }
