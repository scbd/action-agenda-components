    "change-case": "4.1.2",
    "consola": "2.15.3",
    "core-js": "3.6.5",
    "humanparser": "2.6.0",
    "lodash.clone": "4.5.0",
    "vee-validate": "2.2.15",
    "vue-i18n": "8.24.5",
    "vue-multiselect": "2.1.6",
    "vue-recaptcha-v3": "^1.9",
    "vue-upload-component": "2.8.22"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "3.0.0",
